{
  "name": "@flayyer/flayyer-hook",
  "version": "2.3.0",
  "description": "Flayyer.com React Hook to memoize a flayyer instance",
  "keywords": [
    "flayyer",
    "react",
    "react-hook"
  ],
  "main": "dist/flayyer-hook.es5.js",
  "module": "dist/flayyer-hook.esm.js",
  "typings": "dist/flayyer-hook.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "author": "Patricio LÃ³pez Juri <patricio@flayyer.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/flayyer/flayyer-hook.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "rollup -c rollup.config.ts",
    "prepublishOnly": "yarn run build",
    "start": "rollup -c rollup.config.ts -w",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  },
  "dependencies": {},
  "peerDependencies": {
    "@flayyer/flayyer": "^2.3.0",
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@flayyer/eslint-config": "^1.1.1",
    "@flayyer/flayyer": "^2.3.1",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/qs": "^6.9.6",
    "@types/react": "^17.0.8",
    "eslint": "^7.27.0",
    "husky": "^4.3.7",
    "jest": "^27.0.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.5",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
